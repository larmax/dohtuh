    </servlet>
    <servlet-mapping>
        <servlet-name>muokkaaAloitetta</servlet-name>
        <url-pattern>/Servletit/muokkaaaloitetta</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>poistaKayttaja</servlet-name>
